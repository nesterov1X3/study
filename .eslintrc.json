{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:node/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "prettier/prettier": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "prefer-arrow-callback": "off",
        "import/newline-after-import": "off",
        "import/order": "off",
        "consistent-return": "off",
        "camelcase": "off",
        "lines-between-class-members": "off",
        "max-classes-per-file": "off",
        "no-debugger": "off",
        "no-plusplus": "off",
        "no-bitwise": "off"
    }
}